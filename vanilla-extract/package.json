{
  "private": true,
  "sideEffects": [
    "*.css.ts"
  ],
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "tsup",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "@vanilla-extract/css": "^1.9.0",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "clsx": "^1.2.1",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vanilla-extract/esbuild-plugin": "^2.1.0",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
